                                       </Column>
<!--                                 <Column xmlns="sap.ui.table" width="3%" mergeDuplicates="false" > -->
<!--                                	 <Label text="Confidence" xmlns="sap.ui.commons" /> -->
<!--                                	 <template> -->
<!-- 											<commons:TextView text="{overallConfidence}" -->
<!-- 												/> -->
											
<!-- 										</template> -->
<!--                                 </Column> -->
