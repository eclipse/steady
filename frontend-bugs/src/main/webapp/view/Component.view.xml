	                                                	id="sOrigin"
														items="{
															path: 'originModel>/ContentOriginLevel',
															sorter: { path: 'Origin' }
														}">
														<core:Item key="{originModel>Origin}" text="{originModel>Origin}" />
													</mm:ComboBox>                                               
	                                             </l:content>
	                                        </l:HorizontalLayout>
